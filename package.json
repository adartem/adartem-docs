{
  "name": "adartem-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx || echo \"No linting rules applied\"",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "build": "echo \"Building docs\" && mkdir -p build && echo '<!DOCTYPE html><html><head><title>ADARTEM Docs</title></head><body><h1>ADARTEM Documentation</h1></body></html>' > build/index.html",
    "start": "npx serve build -l 3000"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^30.1.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "^3.6.2",
    "serve": "^14.2.3",
    "typescript-eslint": "^8.44.1"
  }
}
